<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/IGapRootViewStyle"
    tools:context=".mobileBank.view.MobileBankHomeFragment">

    <include
        android:id="@+id/lytTop"
        layout="@layout/mobile_bank_top"/>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/vpCards"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp200"
        android:paddingRight="@dimen/dp32"
        android:paddingLeft="@dimen/dp32"
        android:clipToPadding="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lytTop"/>

</androidx.constraintlayout.widget.ConstraintLayout>