<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp48">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/phone_Number"
        style="?iGapSubtitleTextColor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="@dimen/dp16"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:textColor="?iGapSubtitleTextColor"
        app:layout_constraintBottom_toBottomOf="@+id/contact_Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/contact_Name"
        app:layout_constraintTop_toTopOf="@+id/contact_Name"
        app:layout_constraintWidth_percent="5"
        tools:text="09120423503" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/contact_Name"
        style="?iGapSubtitleTextColor"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="@dimen/dp16"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:textColor="?iGapSubtitleTextColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/phone_Number"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="5"
        tools:text="abolfazl" />

    <View
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="?iGapDividerLine"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>