<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CollapsingImageBehavior">
        <attr name="collapsedTarget" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="GifMoviewView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTheme">
        <attr name="gifMoviewViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="RatioImageView">
        <attr name="groupId" format="string" />
        <attr name="adjustWidth" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ArcMenu">
        <attr name="menu_scr" format="integer" />
        <attr name="menu_color" format="reference|color" />
        <attr name="menu_ripple_color" format="color" />
        <attr name="menu_radius" format="dimension" />
        <attr name="menu_margin" format="dimension" />
        <attr name="menu_open" format="enum">
            <enum name="arc_left" value="0" />
            <enum name="arc_right" value="1" />
        </attr>
        <attr name="menu_animation_time" format="integer" />
        <attr name="menu_elevation" format="dimension" />
        <attr name="menu_angle" format="dimension" />
        <attr name="menu_config" format="boolean" />
    </declare-styleable>

    <!--ripple library-->
    <declare-styleable name="RippleView">
        <attr name="rv_alpha" format="integer" />
        <attr name="rv_framerate" format="integer" />
        <attr name="rv_rippleDuration" format="integer" />
        <attr name="rv_zoomDuration" format="integer" />
        <attr name="rv_color" format="color" />
        <attr name="rv_centered" format="boolean" />
        <attr name="rv_type" format="enum">
            <enum name="simpleRipple" value="0" />
            <enum name="doubleRipple" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="rv_ripplePadding" format="dimension" />
        <attr name="rv_zoom" format="boolean" />
        <attr name="rv_zoomScale" format="float" />

    </declare-styleable>

    <!--=======================custom theme=====================-->

    <attr name="iGapButtonStroke" format="reference" />
    <attr name="iGapButton" format="reference" />
    <attr name="iGapButtonGray" format="reference" />
    <attr name="greenTextIconColor" format="reference" />
    <attr name="iGapKuknosLogout" format="reference" />

    <!--setting-->
    <attr name="backgroundColorSettingTheme_2" format="reference" />
    <attr name="lineColorSettingTheme" format="reference" />
    <attr name="hintColorSettingTheme" format="reference" />
    <attr name="textBackgroundColor" format="reference" />
    <attr name="textBackgroundColor_2" format="reference" />
    <attr name="textSecurityColor" format="reference" />
    <attr name="textColorCall" format="reference" />
    <attr name="backgroundColorCall" format="reference" />
    <attr name="emojiChat" format="reference" />
    <attr name="backgroundSubChatTheme" format="reference" />
    <attr name="fabButtoniGap" format="reference" />
    <attr name="backgroundPassCode" format="reference" />

    <!--=======================custom new theme=====================-->

    <attr name="rootBackgroundColor" format="reference" />
    <attr name="iGapTitleTextColor" format="reference" />
    <attr name="iGapSubtitleTextColor" format="reference" />

    <!--=======================custom theme=====================-->


    <!--bottom navigation-->
    <declare-styleable name="BottomNavigation">
        <attr name="background_color" format="color" />
        <attr name="corner_radius" format="integer" />
    </declare-styleable>

    <declare-styleable name="TabItem">
        <attr name="selected_icon" format="reference" />
        <attr name="unselected_icon" format="reference" />
        <attr name="dark_selected_icon" format="reference" />
        <attr name="dark_unselected_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="PlaqueView">
        <attr name="pv_edit_mode" format="boolean" />
    </declare-styleable>

    <!--circle-->
    <declare-styleable name="CircleSeekBar">
        <attr name="csb_progress" format="integer" />
        <attr name="csb_min" format="integer" />
        <attr name="csb_max" format="integer" />
        <attr name="csb_step" format="integer" />

        <attr name="csb_thumbDrawable" format="reference" />
        <attr name="csb_thumbSize" format="dimension" />

        <attr name="csb_progressColor" format="color" />
        <attr name="csb_progressWidth" format="dimension" />

        <attr name="csb_arcColor" format="color" />
        <attr name="csb_arcWidth" format="dimension" />

        <attr name="csb_textSize" format="dimension" />
        <attr name="csb_textColor" format="color" />

        <attr name="csb_isShowText" format="boolean" />

    </declare-styleable>

    <declare-styleable name="AudioWave">
        <attr name="waveColor" format="color" />
        <attr name="chunkWidth" format="dimension" />
        <attr name="chunkHeight" format="dimension" />
        <attr name="minChunkHeight" format="dimension" />
        <attr name="chunkRadius" format="dimension" />
        <attr name="chunkSpacing" format="dimension" />
        <attr name="progress" format="float" />
        <attr name="animateExpansion" format="boolean" />
        <attr name="touchable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BannerSlider">
        <attr name="slider_hideIndicators" format="boolean" />
        <attr name="slider_indicatorSize" format="dimension" />
        <attr name="slider_selectedSlideIndicator" format="reference" />
        <attr name="slider_unselectedSlideIndicator" format="reference" />
        <attr name="slider_animateIndicators" format="boolean" />
        <attr name="slider_interval" format="integer" />
        <attr name="slider_loopSlides" format="boolean" />
        <attr name="slider_defaultBanner" format="integer" />
        <attr name="slider_emptyView" format="reference" />
    </declare-styleable>
</resources>
